<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.note.model.Todo" table="notes">
		<id name="id" column="notesId">
			<generator class="increment" />
		</id>
		
		<property name="userId" column="fk_user_id"  type="int"></property>
		<property name="notes" type="string"></property>
		<property name="createDate" type="java.util.Date"></property>
		<property name="updateDate" type="java.util.Date"></property>
		<property name="searchDate" type="date"></property>
	</class>

</hibernate-mapping>